{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node src/server.ts",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset",
    "prisma:seed": "tsx prisma/seed/index.ts",
    "prisma:seed:upsert": "tsx prisma/seed/index.ts --upsert",
    "clean": "rm -rf dist",
    "copy-engine": "mkdir -p dist && cp prisma/generated/client/libquery_engine-debian-openssl-3.0.x.so.node dist/query-engine.so.node",
    "copy-prisma-client": "tsx copy-prisma-client.ts",
    "build": "pnpm run clean && pnpm prisma generate && npm run copy-prisma-client && tsc && pnpm run copy-engine",
    "start": "node dist/server.js",
    "postinstall": "prisma generate",
    "db:push": "prisma db push --force-reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@pothos/core": "^4.6.1",
    "@pothos/plugin-prisma": "^4.7.0",
    "@pothos/plugin-relay": "^4.4.1",
    "@pothos/plugin-simple-objects": "^4.1.1",
    "@prisma/client": "^6.6.0",
    "@types/xlsx": "^0.0.36",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "express": "^5.0.1",
    "express-async-handler": "^1.2.0",
    "fast-xml-parser": "^5.2.0",
    "fs-extra": "^11.3.0",
    "graphql": "^16.10.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-scalars": "^1.24.2",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-serve-static-core": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.1",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
